{
  "name": "livingroom-menubar",
  "version": "0.1.1",
  "author": "Kla Haeck",
  "description": "control the livingromm music from the menubar",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "electron-is-dev": "^0.3.0",
    "electron-updater": "^2.21.4",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-scripts": "1.1.4",
    "react-socket-io": "^0.2.4",
    "reactstrap": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-pack": "electron-builder",
    "preelectron-pack": "yarn build",
    "electron-ship": "electron-builder -p always",
    "preelectron-ship": "yarn build"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "electron": "^1.8.4",
    "electron-builder": "^20.8.1",
    "wait-on": "^2.1.0"
  },
  "build": {
    "appId": "com.fallon.livingroom-menu",
    "productName": "Livingroom Menu",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.productivity",
      "identity": "Mac Developer: username (klahaeck)",
      "target": [
        "dmg"
      ]
    }
  }
}
